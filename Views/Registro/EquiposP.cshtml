
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <!--Boostrap-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>

    <!-- Bootstrap DatePicker -->
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/css/bootstrap-datepicker.css"
          type="text/css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.js"
            type="text/javascript"></script>

    <!-- Font Awesome for icons -->
    <script src="https://kit.fontawesome.com/14cfd73b7f.js" crossorigin="anonymous"></script>
    <!-- ===== CSS ===== -->
    <link rel="stylesheet" href="~/CSS/Registro/EquiposP.css">
</head>


<body>

    <div class="contenedor">
        <section class="head-general">
            <div class="title-text">
                <div class="title">
                    <h2 class="">Registro de Equipos</h2>
                </div>
                <div class="nav-group justify-content-between">

                    <main>
                        <nav class="tab-link">
                            <a href="#" data-url="@Url.Action("E_Impresoras", "Registro")">Impresoras</a>
                            <a href="#" data-url="@Url.Action("E_Laptops", "Registro")">Laptop</a>
                            <a href="#" data-url="@Url.Action("E_Computadoras", "Registro")">Computadoras</a>
                        </nav>
                    </main>


                    <div id="contadorInactividad" class="title-inactivity">
                        <span id="tiempoRestante">Tiempo de cierre por inactividad: 03:00</span>
                    </div>
                </div>
            </div>

        </section>
        <!-- Content Area -->
        <div id="content-area">
            <!-- Default Content (optional) -->
            <div class="content">
                <p>Seleccione una opción del menú para cargar el contenido.</p>
            </div>
        </div>
    </div>


    <!-- Modal Cargando -->
    <div id="modalProcesando" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-body d-flex align-items-center">
                    <img class="ima-pros" src="~/Image/PF_sinfondo.png" />
                    <span class="ml-4">Procesando...</span>
                </div>
            </div>
        </div>
    </div>



    <script src="~/JS/Registro/EquiposP.js"></script>
    <script src="~/JS/Utils.js"></script>
    <script src="~/JS/Errores.js"></script>
</body>